/dts-v1/;
/plugin/;
 
/ {
    compatible = "brcm,bcm2708";
    fragment@0 {
        target = <&spi0>;
        __overlay__ {
            spidev@0 {
                status = "disabled";
            };
            sd1: sd@1 {
				reg = <0>;
                #address-cells = <1>;
                #size-cells = <0>;
				status = "okay";
				compatible = "spi,mmc_spi";
				voltage-ranges = <3200 3500>;
				spi-max-frequency = <8000000>;
            };
        };
    };
};
